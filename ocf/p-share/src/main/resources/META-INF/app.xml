<?xml version="1.0"?>
<app xmlns="http://www.multiable.com/app">
	<name>ce01_ocf</name>
	<version>1.0.0</version>
	<description>OCF</description>
	<dependences>
		<dependence id="caw" versions="1.0" />
		<dependence id="ce01_root" versions="1.0" />
		<dependence id="ce01_core" versions="1.0" />
		<dependence id="ce01_trdg" versions="1.0" />
	</dependences>	
	<ear>caw_ear</ear>
	<ejb>ce01_ocf_ejb</ejb>
	<extension>
		<view code="pro" app="ce01_core" filter="" type="frame" apDebug="true">
			<page>/view/opcq/extension/opcqCardsizeExtension.xhtml</page>
		</view>
		<view code="dn" app="ce01_core" filter="" type="frame" apDebug="true">
			<page>/view/opcq/extension/opcqDNExtension.xhtml</page>
			<listener>com.multiable.opcq.bean.view.listener.OpcqDNListener</listener>
		</view>
		<view code="account" app="ce01_core" filter="" type="frame" apDebug="true">
			<page>/view/opcq/extension/opcqChaccExtension.xhtml</page>
		</view>
		<view code="cus" app="ce01_core" filter="" type="frame" apDebug="true">
			<page>/view/opcq/extension/opcqRemcusExtension.xhtml</page>
			<listener>com.multiable.opcq.bean.view.listener.OpcqRemcusListener</listener>
		</view>
		<view code="siso" app="ce01_trdg" filter="" type="frame" apDebug="true">
			<listener>com.multiable.opcq.bean.view.listener.OpcqSalesInvoiceListener</listener>
		</view>
		<view code="recReg" app="ce01_trdg" filter="" type="frame" apDebug="true">
			<listener>com.multiable.opcq.bean.view.listener.OpcqReceiptRegisterListener</listener>
		</view>
	</extension>	
</app>
